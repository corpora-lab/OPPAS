/*  Corresponding procedure in C syntax:

	void quicksort (int left,int right)
	{
		int lo,hi,piv;
		if (left >= right) return;
		piv = a[right]; lo = left; hi = right;
		while (lo <= hi) {
			if (a[hi] > piv) {
				hi--;
			} else {
				swap a[lo],a[hi];
				lo++;
			}
		}
		quicksort(left,hi);
		quicksort(lo,right);
	}	

	Ref: Schwoon, Stefan. Model-checking pushdown systems. Diss. Technische Universität München, 2002. 
	We consider an array of 2 elements.
*/

formulas = 	F (ret And main),
			XNu (ret And main),
			F ( G (okay)),
			XNu (okay),
			F (ret And main And (okay)),
			XNu (ret And main And (okay));


program:
bool  okay;
s8 tmp, left, right, lo, hi, piv, swapLeft, swapRight;
s8[5] a;


main() {
	left = 0s8;
	right = 4s8;
	a[0s8] = *;
	a[1s8] = *;
	a[2s8] = *;
	a[3s8] = *;
	a[4s8] = *;
	qs();
}

qs() {
	if (left >= right) {
	} else {
		piv = a[right]; 
		lo = left;
		hi = right;
		while (lo <= hi) { 
			qsif();
		}

		// recursive calls
		if (lo == 0s8 && right == 1s8){
			qsRec0();
		} else {
			if (lo == 0s8 && right == 2s8){
				qsRec1();
			} else {
				if (lo == 0s8 && right == 3s8){
					qsRec2();
				} else {
					if (lo == 0s8 && right == 4s8){
						qsRec3();
					} else {
						if (lo == 1s8 && right == 2s8){
							qsRec4();
						} else {
							if (lo == 1s8 && right == 3s8){
								qsRec5();
							} else {
								if (lo == 1s8 && right == 4s8){
									qsRec6();
								} else {
									if (lo == 2s8 && right == 3s8){
										qsRec7();
									} else {
										if (lo == 2s8 && right == 4s8){
											qsRec8();
										} else {
											if (lo == 3s8 && right == 4s8){
												qsRec9();
											} else {
												qsRec10();
											}
										}
									}
								}
							}
						}
					}
				}
			} 
		}
	}
}

qsif() { 
	if (a[hi] > piv){
		hi = hi - 1s8; 
	} else {
		swapLeft = lo;
		swapRight = hi;
		swapElements();
		lo = lo + 1s8;
	}
}

swapElements() {
	tmp = a[swapLeft];
	a[swapLeft] = a[swapRight];
	a[swapRight] = tmp;
	okay = a[0s8] <= a[1s8] && a[1s8] <= a[2s8] && a[2s8] <= a[3s8] && a[3s8] <= a[4s8];
}

//recursive cases depending on the value of lo and right for a five-elements array
qsRec0() {
	right = hi;
	qs();
	left = 0s8; 
	right = 1s8;
	qs();
}

qsRec1() {
	right = hi;
	qs();
	left = 0s8; 
	right = 2s8;
	qs();
}

qsRec2() {
	right = hi;
	qs();
	left = 0s8; 
	right = 3s8;
	qs();
}

qsRec3() {
	right = hi;
	qs();
	left = 0s8; 
	right = 4s8;
	qs();
}

qsRec4() {
	right = hi;
	qs();
	left = 1s8; 
	right = 2s8;
	qs();
}

qsRec5() {
	right = hi;
	qs();
	left = 1s8; 
	right = 3s8;
	qs();
}

qsRec6() {
	right = hi;
	qs();
	left = 1s8; 
	right = 4s8;
	qs();
}

qsRec7() {
	right = hi;
	qs();
	left = 2s8; 
	right = 3s8;
	qs();
}

qsRec8() {
	right = hi;
	qs();
	left = 2s8; 
	right = 4s8;
	qs();
}


qsRec9() {
	right = hi;
	qs();
	left = 3s8; 
	right = 4s8;
	qs();
}

qsRec10() {
	right = hi;
	qs();
	//abort second recursive call, it involves at most one element
	//qs();
}