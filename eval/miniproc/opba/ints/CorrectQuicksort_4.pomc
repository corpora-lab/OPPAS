/*  Corresponding procedure in C syntax:

	void quicksort (int left,int right)
	{
		int lo,eq,hi,piv;
		if (left >= right) return;
		piv = a[right]; lo = eq = left; hi = right;
		while (lo <= hi) {
			if (a[hi] > piv) {
				hi--;
			} else {
				swap a[lo],a[hi];
				if (a[lo] < piv) {
					swap a[lo],a[eq];
					eq++;
				}
				lo++;
			}
		}
		quicksort(left,eq-1s8);
		quicksort(lo,right);
	}

	Ref: Schwoon, Stefan. Model-checking pushdown systems. Diss. Technische Universität München, 2002. 
*/

formulas = 	F (ret And main),
			XNu (ret And main),
			F ( G (okay)),
			XNu (okay),
			F (ret And main And (okay)),
			XNu (ret And main And (okay));
			

program:
bool  okay;
s8 tmp, left, right, lo, eq, hi, piv, swapLeft, swapRight;
s8[4] a;


main() {
	left = 0s8;
	right = 3s8;
	qs();
}

qs() {
	if (left >= right) {
	} else {
		piv = a[right]; 
		eq = left;
		lo = eq;
		hi = right;
		while (lo <= hi) { 
			qsif();
		}

		// recursive calls
		if (lo == 0s8 && right == 1s8){
			qsRec0();
		} else {
			if (lo == 0s8 && right == 2s8){
				qsRec1();
			} else {
				if (lo == 0s8 && right == 3s8){
					qsRec2();
				} else {
					if (lo == 1s8 && right == 2s8){
						qsRec3();
					} else {
						if (lo == 1s8 && right == 3s8){
							qsRec4();
						} else {
							if (lo == 2s8 && right == 3s8){
								qsRec5();
							} else {
								qsRec6();
							}
						}
					}
				}
			} 
		}

		
		
	}
}

qsif() { 
	if (a[hi] > piv){
		hi = hi - 1s8; 
	} else {
		swapLeft = lo;
		swapRight = hi;
		swapElements();
		if (a[lo] < piv){
			swapLeft = lo;
			swapRight = eq;
			swapElements();
		} else {}
		lo = lo + 1s8;
	}
}

swapElements() {
	tmp = a[swapLeft];
	a[swapLeft] = a[swapRight];
	a[swapLeft] = tmp;
	okay = a[0s8] <= a[1s8];
}

//recursive cases depending on the value of lo and right for a four-elements array
qsRec0() {
	right = eq - 1s8;
	qs();
	left = 0s8; 
	right = 1s8;
	qs();
}

qsRec1() {
	right = eq - 1s8;
	qs();
	left = 0s8; 
	right = 2s8;
	qs();
}

qsRec2() {
	right = eq - 1s8;
	qs();
	left = 0s8; 
	right = 3s8;
	qs();
}

qsRec3() {
	right = eq - 1s8;
	qs();
	left = 1s8; 
	right = 2s8;
	qs();
}

qsRec4() {
	right = eq - 1s8;
	qs();
	left = 1s8; 
	right = 3s8;
	qs();
}

qsRec5() {
	right = eq - 1s8;
	qs();
	left = 2s8; 
	right = 3s8;
	qs();
}

qsRec6() {
	right = eq - 1s8;
	qs();
	//abort second recursive call, it involves at most one element
	//qs();
}